import QmlProject

Project {
	mainUiFile: "Home/Default.ui.qml"

	QmlFiles {
		directory: "."
	}

	ImageFiles {
		directory: "Font-Awesome"
	}

	ImageFiles {
		directory: "Bootstrap"
	}

	Files {
		filter: "*.conf"
		files: ["qtquickcontrols2.conf"]
	}

	Files {
		filter: "*.ttf"
	}

	Environment {
		QT_QUICK_CONTROLS_CONF: "qtquickcontrols2.conf"
	}

	qt6Project: true

	importPaths: [ "imports" ]
}
